{
  "files.associations": {
    ".env*": "plaintext"
  },
  "github.copilot.enable": {
    "plaintext": false,
    "markdown": true,
    "scminput": false,
  },
  "github.copilot.advanced": {
    "ignoreFiles": [
      "**/.env*",
      ".env",
      "**/.env.local",
      "**/.env.development",
      "**/.env.production",
      "**/api/.env*"
    ]
  },
}
